{
  "schemaVersion": 1,
  "id": "apugli",
  "version": "${version}",
  "name": "Apugli",
  "description": "Pug's extension to Origins for powers.",
  "authors": [
    "Pug"
  ],
  "contact": {
    "sources": "https://github.com/MerchantPug/apugli"
  },
  "license": "MIT",
  "icon": "assets/apugli/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.merchantpug.apugli.fabric.ApugliFabric"
    ],
    "client": [
      "io.github.merchantpug.apugli.fabric.ApugliFabricClient"
    ]
  },
  "accessWidener": "apugli.accessWidener",
  "mixins": [
    "apugli.mixins.json",
    "apugli.mixins.fabric.json"
  ],
  "custom": {
    "modmenu": {
      "badges": ["library"]
    }
  },
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": ">=1.16.4",
    "architectury": ">=1.10.142",
    "origins": ">=0.7.3"
  },
  "breaks": {
    "origins-power-expansion": "*"
  }
}