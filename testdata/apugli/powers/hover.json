{
  "type": "origins:multiple",
  "hover": {
    "type": "apugli:hover",
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "*:*_active_resource",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "apugli:velocity",
          "y": 0.0,
          "comparison": ">="
        },
        {
          "type": "apugli:key_pressed",
          "key": {
            "key": "key.jump",
            "continuous": true
          }
        },
        {
          "type": "origins:climbing",
          "inverted": true
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": "0",
          "inverted": true
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:lava",
          "comparison": ">",
          "compare_to": "0",
          "inverted": true
        },
        {
          "type": "origins:fall_flying",
          "inverted": true
        },
        {
          "type": "apugli:riding",
          "inverted": true
        },
        {
          "type": "origins:power_active",
          "power": "*:*_toggle"
        }
      ]
    }
  },
  "deplete_while_using": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:change_resource",
          "resource": "*:*_active_resource",
          "change": -1
        },
        {
          "type": "apugli:set_resource",
          "resource": "*:*_recharge_condition_resource",
          "value": 0
        }
      ]
    },
    "interval": 1,
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_hover"
    }
  },
  "recharge": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_active_resource",
      "change": 2
    },
    "interval": 1,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:power_active",
          "power": "*:*_hover",
          "inverted": true
        },
        {
          "type": "origins:resource",
          "resource": "*:*_recharge_condition_resource",
          "comparison": "==",
          "compare_to": 10
        }
      ]
    }
  },
  "recharge_condition_increase": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:change_resource",
      "resource": "*:*_recharge_condition_resource",
      "change": 1
    },
    "interval": 1,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:or",
          "conditions": [
            {
              "type": "origins:on_block"
            },
            {
              "type": "apugli:riding"
            }
          ]
        },
        {
          "type": "origins:power_active",
          "power": "*:*_hover",
          "inverted": true
        }
      ]
    }
  },
  "active_resource": {
    "type": "origins:resource",
    "min": 0,
    "max": 60,
    "start_value": 60,
    "hud_render": {
      "should_render": true,
      "sprite_location": "apugli:tmo_resource_bar.png",
      "bar_index": 8,
      "condition": {
        "type": "origins:power_active",
        "power": "*:*_toggle"
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  },
  "inactive_resource": {
    "type": "origins:resource",
    "min": 0,
    "max": 60,
    "hud_render": {
      "should_render": true,
      "sprite_location": "apugli:tmo_resource_bar.png",
      "bar_index": 9,
      "condition": {
        "type": "origins:power_active",
        "power": "*:*_toggle",
        "inverted": true
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle",
      "inverted": true
    }
  },
  "recharge_condition_resource": {
    "type": "origins:resource",
    "min": 0,
    "max": 10,
    "hud_render": {
      "should_render": false
    }
  },
  "sync_values": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "apugli:resource_transfer",
      "resource": "*:*_inactive_resource",
      "provider": "*:*_active_resource"
    },
    "interval": 1,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "apugli:compare_resource",
          "resource": "*:*_inactive",
          "compare_to": "*:*_inactive_resource",
          "comparison": "==",
          "inverted": true
        },
        {
          "type": "origins:power_active",
          "power": "*:*_toggle",
          "inverted": true
        }
      ]
    }
  },
  "toggle_key": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:power_active",
        "power": "*:*_toggle"
      },
      "if_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "apugli:toggle",
            "power": "*:*_toggle"
          },
          {
            "type": "apugli:resource_transfer",
            "resource": "*:*_inactive_resource",
            "provider": "*:*_active_resource"
          }
        ]
      },
      "else_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "apugli:toggle",
            "power": "*:*_toggle"
          },
          {
            "type": "apugli:resource_transfer",
            "resource": "*:*_active_resource",
            "provider": "*:*_inactive_resource"
          }
        ]
      }
    },
    "cooldown": 1,
    "hud_render": {
      "should_render": false
    },
    "key": {
      "key": "key.saveToolbarActivator"
    }
  },
  "toggle": {
    "type": "origins:toggle",
    "active_by_default": true,
    "key": {
      "key": "key.thisdoesntexist"
    }
  }
}